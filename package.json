{
  "name": "Moira",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:integralads/Moira.git"
  },
  "description": "A browser plug-in that displays information from Integral tags on live web pages.",
  "main": "",
  "author": "Daniel A Deychakiwsky",
  "license": "ISC",
  "scripts": {
    "build": "node_modules/browserify/bin/cmd.js MainDog.js -o chrome-extension/bundle.js"
  },
  "devDependencies": {
    "browserify-shim": "^3.8.2",
    "grunt": "^0.4.5",
    "grunt-browserify": "^4.0.1",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "handlebars": "^4.0.5",
    "hbsfy": "^2.5.1"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "backbone.marionette": "^2.3.1",
    "backbone.wreqr": "^1.3.1",
    "browserify": ">= 2.3.0 < 9",
    "http-server": "0.8.5",
    "jquery": "^2.1.3",
    "materialize-css": "^0.97.5",
    "underscore": "^1.7.0"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.min.js",
    "underscore": "./node_modules/underscore/underscore-min.js",
    "backbone": "./node_modules/backbone/backbone-min.js",
    "backbone.marionette": "./node_modules/backbone.marionette/lib/backbone.marionette.min.js",
    "backbone.wreqr": "./node_modules/backbone.wreqr/lib/backbone.wreqr.min.js",
    "materialize-js": "./node_modules/materialize-css/dist/js/materialize.min.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:$",
        "underscore:_"
      ]
    },
    "materialize-js": {
      "exports": "Materialize",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "backbone.marionette": {
      "exports": "Marionette",
      "depends": [
        "jquery:$",
        "backbone:Backbone",
        "underscore:_"
      ]
    },
    "backbone.wreqr": {
      "exports": "Backbone.Wreqr",
      "depends": [
        "backbone:Backbone"
      ]
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  }
}
