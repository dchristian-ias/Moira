{
  "manifest_version": 2,

  "name": "Dog Diagnostic Dashboard",
  "description": "Dashboard for viewing integral collection",
  "version": "1.0",
  "content_scripts" : [{
    "matches":["http://*/*","https://*/*"],
    "css":["style.css"],
    "js":["Bootstrapper.js","bundle.js"]
  }],
  "browser_action": {
    "default_icon": "dogD.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "proxy"
  ]
}